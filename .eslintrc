{
  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "node": true,
  },
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "globals": {
    "jQuery": true,
  },
  "rules": {
    "arrow-body-style": 0,
    "default-case": 0,
    "id-length": [2, {"min": 2, "properties": "never", "exceptions": ["a", "b", "c", "d", "e", "f", "i", "j", "k", "r", "s", "x", "y", "z", "$"]}],
    "indent": [2 ,2, {"SwitchCase": 1}],
    "max-len": [1, 160, 2, {"ignoreComments": true}],
    "no-cond-assign": [2, "except-parens"],
    "no-param-reassign": 0,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "object-curly-spacing": [2, "never"],
    "quote-props": [2, "consistent-as-needed"],
  }
}
