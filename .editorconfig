# EditorConfig: http://editorconfig.org
# Global EditorConfig
# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.css]
insert_final_newline = false

[*.md]
trim_trailing_whitespace = false
indent_size = 4

[*.svg]
insert_final_newline = false

[LICENSE]
insert_final_newline = false
